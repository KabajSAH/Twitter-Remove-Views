{
	"manifest_version" : 3,
	"name": "Remove Twitter Views",
	"description": "Remove the Views button from Twitter",
	"version": "1.2.2",
	"author" : "Kabaj",
	"content_scripts": [
		{
			"matches": ["*://twitter.com/*"],
			"js":["./scripts/content.js"]
		}
	],
	"icons": {
			"16": "images/icon-16.png",
			"32": "images/icon-32.png",
			"64": "images/icon-64.png",
			"128": "images/icon-128.png"
		}
}
